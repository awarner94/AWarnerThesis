	author = {Binghai Yan and Shou-Cheng Zhang},
	title = {Topological materials},
	journal = {Reports on Progress in Physics},
	abstract = {Recently, topological insulator materials have been theoretically predicted and experimentally observed in both 2D and 3D systems. We first review the basic models and physical properties of topological insulators, using HgTe and Bi2Se3 as prime examples. We then give a comprehensive survey of topological insulators which have been predicted so far, and discuss the current experimental status.}
}


