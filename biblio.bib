@Article{hoffman_2011-review,
	doi = {10.1088/0034-4885/74/12/124513},
	url = {https://doi.org/10.1088/0034-4885/74/12/124513},
	year = 2011,
	month = {nov},
	publisher = {{IOP} Publishing},
	volume = {74},
	number = {12},
	pages = {124513},
	author = {Jennifer E Hoffman},
	title = {Spectroscopic scanning tunneling microscopy insights into Fe-based superconductors},
	journal = {Reports on Progress in Physics},
	abstract = {In the first three years since the discovery of Fe-based high Tc superconductors, scanning tunneling microscopy (STM) and spectroscopy have shed light on three important questions. First, STM has demonstrated the complexity of the pairing symmetry in Fe-based materials. Phase-sensitive quasiparticle interference (QPI) imaging and low temperature spectroscopy have shown that the pairing order parameter varies from nodal to nodeless sÂ± within a single family, FeTe1âˆ’xSex. Second, STM has imaged C4 â†’ C2 symmetry breaking in the electronic states of both parent and superconducting materials. As a local probe, STM is in a strong position to understand the interactions between these broken symmetry states and superconductivity. Finally, STM has been used to image the vortex state, giving insights into the technical problem of vortex pinning, and the fundamental problem of the competing states introduced when superconductivity is locally quenched by a magnetic field. Here we give a pedagogical introduction to STM and QPI imaging, discuss the specific challenges associated with extracting bulk properties from the study of surfaces, and report on progress made in understanding Fe-based superconductors using STM techniques.}
}


